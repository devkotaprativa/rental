<!DOCTYPE html>
<html>
<head>
  <title>Rental</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% if flash[:msg] %>
			<div class  = "alert alert-warning">
			<%= flash[:msg]%>
		</div>
		<% end %>

	<%= yield %>

	<%= render partial: "layouts/footer"%>

</body>
</html>
